{
  "expo": {
    "name": "ToDo-App",
    "slug": "ToDo-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icono.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0175b0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.leancba.TodoApp", "versionCode" : 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}